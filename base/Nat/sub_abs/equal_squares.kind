          let h = Nat.reflexivity(Nat.pow(Nat.succ(a.pred),2) + 0)
          let h1 = h :: rewrite X in ((Nat.pow(Nat.succ(a.pred),2) + X) == (Nat.pow(Nat.succ(a.pred),2)+0)) with mirror(Nat.mul.zero_right(2))
